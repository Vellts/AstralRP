    <file src="models/vehicles/hdfbl.dff"/>
    <file src="models/vehicles/hdfbl.txd"/>
    <file src="models/vehicles/ymxt660.dff"/>
    <file src="models/vehicles/ymxt660.txd"/>
    <file src="models/vehicles/dtdmcrr.dff"/>
    <file src="models/vehicles/dtdmcrr.txd"/>
    <file src="models/vehicles/dtdv.dff"/>
