    <file src="models/vehicles/dcrt1970.dff"/>
    <file src="models/vehicles/dcrt1970.txd"/>
    <file src="models/vehicles/dcsrth.dff"/>
    <file src="models/vehicles/dcsrth.txd"/>
    <file src="models/vehicles/phttu.dff"/>
    <file src="models/vehicles/phttu.txd"/>
    <file src="models/vehicles/wmlh.dff"/>
    <file src="models/vehicles/wmlh.txd"/>
    <file src="models/vehicles/ff550.dff"/>
    <file src="models/vehicles/ff550.txd"/>
    <file src="models/vehicles/flferrari.dff"/>
    <file src="models/vehicles/flferrari.txd"/>
    <file src="models/vehicles/pcgt.dff"/>
    <file src="models/vehicles/pcgt.txd"/>
